{
    "name": "symfony/website-skeleton",
    "type": "project",
    "license": "proprietary",
    "description": "A skeleton to start a new Symfony website",
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "^1.0@dev",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "2.4.x-dev",
        "doctrine/doctrine-migrations-bundle": "3.1.x-dev",
        "doctrine/orm": "^2.9",
        "easycorp/easyadmin-bundle": "^3.2",
        "friendsofsymfony/elastica-bundle": "v6.0.0-beta4",
        "knplabs/knp-paginator-bundle": "5.x-dev",
        "mailjet/mailjet-apiv3-php": "dev-master",
        "phpdocumentor/reflection-docblock": "^5.2",
        "sensio/framework-extra-bundle": "^6.1",
        "symfony/asset": "5.3.*",
        "symfony/console": "5.3.*",
        "symfony/dotenv": "5.3.*",
        "symfony/expression-language": "5.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.3.*",
        "symfony/framework-bundle": "5.3.*",
        "symfony/http-client": "5.3.*",
        "symfony/intl": "5.3.*",
        "symfony/mailer": "5.3.*",
        "symfony/messenger": "5.3.*",
        "symfony/mime": "5.3.*",
        "symfony/monolog-bundle": "3.x-dev",
        "symfony/notifier": "5.3.*",
        "symfony/process": "5.3.*",
        "symfony/property-access": "5.3.*",
        "symfony/property-info": "5.3.*",
        "symfony/proxy-manager-bridge": "5.3.*",
        "symfony/runtime": "5.3.*",
        "symfony/security-bundle": "5.3.*",
        "symfony/serializer": "5.3.*",
        "symfony/string": "5.3.*",
        "symfony/translation": "5.3.*",
        "symfony/twig-bundle": "^5.3@RC",
        "symfony/ux-chartjs": "1.3.x-dev",
        "symfony/ux-turbo-mercure": "1.3.x-dev",
        "symfony/validator": "5.3.*",
        "symfony/web-link": "5.3.*",
        "symfony/webpack-encore-bundle": "v1.11.1",
        "symfony/yaml": "5.3.*",
        "symfonycasts/reset-password-bundle": "dev-main",
        "symfonycasts/verify-email-bundle": "dev-main",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "vich/uploader-bundle": "1.x-dev"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "dbrekelmans/bdi": "^0.3.0",
        "doctrine/doctrine-fixtures-bundle": "^3.5.x-dev",
        "easycorp/easy-log-handler": "dev-master",
        "phpspec/prophecy": "^1.13",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "4.0.x-dev",
        "symfony/browser-kit": "^5.3@RC",
        "symfony/css-selector": "^5.3@RC",
        "symfony/debug-bundle": "^5.3@RC",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/stopwatch": "^5.3@RC",
        "symfony/var-dumper": "^5.3@RC",
        "symfony/web-profiler-bundle": "^5.3@RC",
        "zenstruck/browser": "^0.4.0",
        "zenstruck/foundry": "^1.9"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.3.*"
        }
    }
}
